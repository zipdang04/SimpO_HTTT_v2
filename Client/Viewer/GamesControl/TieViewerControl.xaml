        <compo:QNPBoxV2 x:Name="qBox"/>
        <Rectangle x:Name="rectBell1" HorizontalAlignment="Left" Height="40" Margin="141,553,0,0" VerticalAlignment="Top" Width="261" Fill="#59FF0000"/>
        <Rectangle x:Name="rectBell2" HorizontalAlignment="Left" Height="40" Margin="403,553,0,0" VerticalAlignment="Top" Width="261" Fill="#59FF0000"/>
        <Rectangle x:Name="rectBell3" HorizontalAlignment="Left" Height="40" Margin="665,553,0,0" VerticalAlignment="Top" Width="261" Fill="#59FF0000"/>
        <Rectangle x:Name="rectBell4" HorizontalAlignment="Left" Height="40" Margin="927,553,0,0" VerticalAlignment="Top" Width="261" Fill="#59FF0000"/>
    </Grid>
</UserControl>
