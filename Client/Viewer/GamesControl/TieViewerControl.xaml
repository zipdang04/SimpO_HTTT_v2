             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Client.Viewer.GamesControl"
             xmlns:compo="clr-namespace:Client.Viewer.GamesControl.Components"
             mc:Ignorable="d" 
             d:DesignHeight="900" d:DesignWidth="1600">
    <Grid>
        <!--
        <Grid.Background> <ImageBrush ImageSource="../../Resources/Effects/VD_QuesBackground.png"/> </Grid.Background>
        -->
        <MediaElement x:Name="mediaStart" Source="../../Resources/Effects/CHP_Start.mp4" MediaEnded="mediaStart_MediaEnded" LoadedBehavior="Manual"/>
